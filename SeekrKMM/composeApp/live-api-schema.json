{
    "components": {
        "schemas": {
            "CTA": {
                "additionalProperties": false,
                "properties": {
                    "action": {
                        "type": "string"
                    },
                    "button_text": {
                        "type": "string"
                    },
                    "headline": {
                        "type": "string"
                    },
                    "option": {
                        "type": "string"
                    }
                },
                "required": [
                    "headline",
                    "button_text",
                    "option",
                    "action"
                ],
                "type": [
                    "object",
                    "null"
                ]
            },
            "Error": {
                "additionalProperties": false,
                "properties": {
                    "code": {
                        "format": "int64",
                        "type": "integer"
                    },
                    "message": {
                        "type": "string"
                    }
                },
                "required": [
                    "code",
                    "message"
                ],
                "type": "object"
            },
            "Image": {
                "additionalProperties": false,
                "properties": {
                    "src": {
                        "description": "Image source url.",
                        "type": "string"
                    }
                },
                "required": [
                    "src"
                ],
                "type": "object"
            },
            "ImageSizes": {
                "additionalProperties": false,
                "properties": {
                    "full": {
                        "$ref": "#/components/schemas/Image"
                    },
                    "thumbnail": {
                        "$ref": "#/components/schemas/Image"
                    }
                },
                "required": [
                    "thumbnail",
                    "full"
                ],
                "type": "object"
            },
            "NowPlayingItem": {
                "additionalProperties": false,
                "properties": {
                    "album": {
                        "type": "string"
                    },
                    "artist": {
                        "type": "string"
                    },
                    "image": {
                        "$ref": "#/components/schemas/ImageSizes"
                    },
                    "title": {
                        "type": "string"
                    }
                },
                "required": [
                    "title",
                    "artist",
                    "album",
                    "image"
                ],
                "type": "object"
            },
            "NowPlayingResponseWrapper": {
                "additionalProperties": false,
                "properties": {
                    "call_letters": {
                        "type": "string"
                    },
                    "cta": {
                        "$ref": "#/components/schemas/CTA"
                    },
                    "now_playing": {
                        "$ref": "#/components/schemas/NowPlayingItem"
                    },
                    "on_air": {
                        "$ref": "#/components/schemas/OnAir"
                    }
                },
                "required": [
                    "call_letters",
                    "now_playing",
                    "on_air"
                ],
                "type": "object"
            },
            "OnAir": {
                "additionalProperties": false,
                "properties": {
                    "description": {
                        "type": "string"
                    },
                    "image": {
                        "$ref": "#/components/schemas/WordpressImageSizes",
                        "description": "An Image object."
                    },
                    "name": {
                        "type": "string"
                    }
                },
                "required": [
                    "name",
                    "image"
                ],
                "type": "object"
            },
            "RecentlyPlayed": {
                "additionalProperties": false,
                "properties": {
                    "album": {
                        "type": [
                            "string",
                            "null"
                        ]
                    },
                    "art_url": {
                        "type": [
                            "string",
                            "null"
                        ]
                    },
                    "artist": {
                        "type": [
                            "string",
                            "null"
                        ]
                    },
                    "created_at": {
                        "format": "date-time",
                        "type": [
                            "string",
                            "null"
                        ]
                    },
                    "high_res_art_url": {
                        "type": [
                            "string",
                            "null"
                        ]
                    },
                    "id": {
                        "format": "int64",
                        "type": "integer"
                    },
                    "itunes_purchase_url": {
                        "type": [
                            "string",
                            "null"
                        ]
                    },
                    "length": {
                        "format": "int64",
                        "type": [
                            "integer",
                            "null"
                        ]
                    },
                    "radio_station": {
                        "type": [
                            "string",
                            "null"
                        ]
                    },
                    "started_at": {
                        "format": "date-time",
                        "type": [
                            "string",
                            "null"
                        ]
                    },
                    "title": {
                        "type": [
                            "string",
                            "null"
                        ]
                    },
                    "updated_at": {
                        "format": "date-time",
                        "type": [
                            "string",
                            "null"
                        ]
                    }
                },
                "required": [
                    "id",
                    "radio_station",
                    "artist",
                    "title",
                    "album",
                    "art_url",
                    "high_res_art_url",
                    "itunes_purchase_url",
                    "length",
                    "started_at",
                    "created_at",
                    "updated_at"
                ],
                "type": "object"
            },
            "WordpressImage": {
                "additionalProperties": false,
                "properties": {
                    "alt": {
                        "description": "Image alt text, used for alt attribute on web.",
                        "type": "string"
                    },
                    "caption": {
                        "description": "Image caption.",
                        "type": "string"
                    },
                    "height": {
                        "description": "Image height.",
                        "format": "int64",
                        "type": "integer"
                    },
                    "id": {
                        "description": "Image internal WordPress ID.",
                        "format": "int64",
                        "type": "integer"
                    },
                    "sizes": {
                        "description": "Image sizes, used for sizes attribute on web.",
                        "type": "string"
                    },
                    "src": {
                        "description": "Image source url.",
                        "type": "string"
                    },
                    "srcset": {
                        "description": "Image source set, used for srcset attribute on web.",
                        "type": "string"
                    },
                    "title": {
                        "description": "Image title, used for the title attribute on web.",
                        "type": "string"
                    },
                    "width": {
                        "description": "Image width.",
                        "format": "int64",
                        "type": "integer"
                    }
                },
                "required": [
                    "id",
                    "src"
                ],
                "type": "object"
            },
            "WordpressImageSizes": {
                "additionalProperties": false,
                "properties": {
                    "full": {
                        "$ref": "#/components/schemas/WordpressImage"
                    },
                    "large": {
                        "$ref": "#/components/schemas/WordpressImage"
                    },
                    "medium": {
                        "$ref": "#/components/schemas/WordpressImage"
                    },
                    "thumbnail": {
                        "$ref": "#/components/schemas/WordpressImage"
                    }
                },
                "required": [
                    "thumbnail",
                    "medium",
                    "large",
                    "full"
                ],
                "type": "object"
            }
        }
    },
    "info": {
        "title": "SeekR Live API",
        "version": "1.0"
    },
    "openapi": "3.1.0",
    "paths": {
        "/service/now_playing": {
            "get": {
                "operationId": "getNowPlaying",
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "items": {
                                        "$ref": "#/components/schemas/NowPlayingResponseWrapper"
                                    },
                                    "type": "array"
                                }
                            }
                        },
                        "description": "OK",
                        "headers": {
                            "Cache-Control": {
                                "schema": {
                                    "type": "string"
                                }
                            },
                            "Last-Modified": {
                                "schema": {
                                    "type": "string"
                                }
                            }
                        }
                    },
                    "default": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Error"
                                }
                            }
                        },
                        "description": "Error"
                    }
                },
                "summary": "Get now playing for all radio stations"
            }
        },
        "/service/{call_letters}/now_playing": {
            "get": {
                "operationId": "getNowPlayingByStation",
                "parameters": [
                    {
                        "description": "Radio Station call letters.",
                        "in": "path",
                        "name": "call_letters",
                        "required": true,
                        "schema": {
                            "description": "Radio Station call letters.",
                            "maxLength": 4,
                            "minLength": 2,
                            "pattern": "[a-zA-Z]",
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/NowPlayingResponseWrapper"
                                }
                            }
                        },
                        "description": "OK"
                    },
                    "default": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Error"
                                }
                            }
                        },
                        "description": "Error"
                    }
                },
                "summary": "Get now playing by radio station using call letters"
            }
        },
        "/service/{call_letters}/recently_played": {
            "get": {
                "parameters": [
                    {
                        "description": "Radio Station call letters.",
                        "in": "path",
                        "name": "call_letters",
                        "required": true,
                        "schema": {
                            "description": "Radio Station call letters.",
                            "maxLength": 4,
                            "minLength": 2,
                            "pattern": "[a-zA-Z]",
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "items": {
                                        "$ref": "#/components/schemas/RecentlyPlayed"
                                    },
                                    "type": "array"
                                }
                            }
                        },
                        "description": "OK"
                    },
                    "default": {
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Error"
                                }
                            }
                        },
                        "description": "Error"
                    }
                },
                "summary": "Get recently played songs by station"
            }
        }
    }
}